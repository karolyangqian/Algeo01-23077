<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.myapp.SPLController">
   <children>
      <VBox prefHeight="15.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToMainMenu" text="Main menu" wrapText="true">
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="412.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="imgview" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label text="Sistem Persamaan Linier" textAlignment="CENTER" wrapText="true" />
            <Button mnemonicParsing="false" onAction="#showImage" text="show" />
         </children>
      </VBox>
   </children>
</VBox>
